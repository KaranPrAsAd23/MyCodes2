#include <stdio.h>

int main() {
    int a = 23, b = 4;

   
    if (a && b) {
        printf("a && b is true\n");
    } else {
        printf("a && b is false\n");
    }

    
    if (a || b) {
        printf("a || b is true\n");
    } else {
        printf("a || b is false\n");
    }

   
    if (!a) {
        printf("!a is true\n");
    } else {
        printf("!a is false\n");
    }

    return 0;
}
